                "display": "inline-block"
              },
              "& when (@checkItemsBg)": {
                ".timeline-item": {
                  "background-color": "@timelineColor"
                }
              },
              ".timeline-element:nth-child(odd)": {
                ".timeline-img-panel": {
                  "margin-right": "2rem"
                },
                ".timeline-item-panel": {
                  "margin-left": "2rem"
                }
              },
              ".timeline-element:nth-child(even)": {
                ".timeline-img-panel": {
                  "margin-left": "2rem"
                },
                ".timeline-item-panel": {
                  "margin-right": "2rem"
                }
              }
            },
            "@media (min-width: 768px)": {
